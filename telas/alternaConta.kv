#:kivy 2.0.0
#:include telas/config.kv 
<AlternaConta>:
    #Essa tela receberá um ScrollView pelo arquivo .py
    FloatLayout:
        Label:
            text: "Suas contas"
            pos_hint: {'y': .9}
            size_hint: 1, .1
    GridLayout:
        cols: 2
        size_hint_y: 0.1
        Button:
            text:'Voltar'
            on_press:
                root.manager.current="principal"
                root.manager.transition.direction="right"
                #left para entrar na página
                #right para sair
        Button:
            text:'Nova Conta'
            on_press:
                root.manager.current="adicionaConta"
                root.manager.transition.direction="left"
                #left para entrar na página
                #right para sair
